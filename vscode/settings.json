{
    "editor.autoClosingQuotes": "never",
    "editor.autoClosingBrackets": "never",
    "terminal.integrated.inheritEnv": false,
    "telemetry.telemetryLevel": "off",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "editor.inlayHints.enabled": "onUnlessPressed",
    "vim.vimrc.enable": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [",", "r"],
            "commands": ["editor.action.rename"]
        },
        {
            "before": [",", "f"],
            "commands": ["editor.action.refactor"]
        }
    ],
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.minimap.enabled": false,
    "editor.hideCursorInOverviewRuler": true,
    "python.analysis.indexing": true,
    "python.languageServer": "Pylance",
    "editor.lineNumbers": "on",
    "window.zoomLevel": 2,
    "terminal.integrated.profiles.osx": {
        "my-tmux": {
            "path": "${env:HOME}/code/dotfiles/vscode/tmux.sh",
            "args": []
        }
    },
    "terminal.integrated.defaultProfile.osx": "my-tmux",
    "terminal.integrated.allowChords": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "notebook.formatOnSave.enabled": true,
    "isort.args": ["--profile", "black"],
    "editor.formatOnSave": true,
    "redhat.telemetry.enabled": false,
    "workbench.preferredLightColorTheme": "Default Light+",
    "workbench.preferredDarkColorTheme": "Default Dark+",
    "window.autoDetectColorScheme": true,
    "workbench.colorTheme": "Default Dark+",
    "breadcrumbs.enabled": true,
    "remote.SSH.defaultExtensions": [
        "amodio.tsl-problem-matcher",
        "charliermarsh.ruff",
        "dbaeumer.vscode-eslint",
        "eamodio.gitlens",
        "esbenp.prettier-vscode",
        "github.vscode-pull-request-github",
        "ms-python.debugpy",
        "ms-python.flake8",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter-renderers",
        "ms-toolsai.vscode-jupyter-cell-tags",
        "ms-toolsai.vscode-jupyter-slideshow",
        "ms-vscode-remote.remote-containers",
        "ms-vscode-remote.remote-ssh",
        "ms-vscode-remote.remote-ssh-edit",
        "ms-vscode-remote.remote-wsl",
        "ms-vscode-remote.vscode-remote-extensionpack",
        "ms-vscode.remote-explorer",
        "ms-vscode.remote-server",
        "redhat.vscode-yaml",
        "svelte.svelte-vscode",
        "vscodevim.vim"
    ],
    "_coo_generated.remote.SSH.defaultExtensions": [
        "amodio.tsl-problem-matcher",
        "charliermarsh.ruff",
        "dbaeumer.vscode-eslint",
        "eamodio.gitlens",
        "esbenp.prettier-vscode",
        "github.vscode-pull-request-github",
        "ms-python.debugpy",
        "ms-python.flake8",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter-renderers",
        "ms-toolsai.vscode-jupyter-cell-tags",
        "ms-toolsai.vscode-jupyter-slideshow",
        "ms-vscode-remote.remote-containers",
        "ms-vscode-remote.remote-ssh",
        "ms-vscode-remote.remote-ssh-edit",
        "ms-vscode-remote.remote-wsl",
        "ms-vscode-remote.vscode-remote-extensionpack",
        "ms-vscode.remote-explorer",
        "ms-vscode.remote-server",
        "redhat.vscode-yaml",
        "svelte.svelte-vscode",
        "vscodevim.vim"
    ],
    "terminal.integrated.copyOnSelection": true,
    "search.globalFindClipboard": true,
    "terminal.integrated.macOptionClickForcesSelection": true,
    "workbench.editorLargeFileConfirmation": 20,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "editor.codeActionsOnSave": {}
}
